{
  "name": "@heaven-dolls/cms",
  "version": "1.0.0",
  "private": true,
  "description": "Content Management System for Heaven Dolls",
  "scripts": {
    "develop": "strapi develop",
    "start": "strapi start",
    "build": "strapi build",
    "strapi": "strapi",
    "export": "strapi export",
    "import": "strapi import",
    "seed": "strapi console -e 'require(\"./scripts/seed\")()'",
    "setup": "node scripts/setup.js",
    "deploy": "npm run build && strapi deploy",
    "console": "strapi console",
    "ts:check": "tsc --noEmit",
    "lint": "eslint . --ext .ts,.js",
    "lint:fix": "eslint . --ext .ts,.js --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:api": "jest --testPathPattern=tests/api",
    "test:models": "jest --testPathPattern=tests/models",
    "test:endpoints": "jest --testPathPattern=tests/api/endpoints",
    "test:webhooks": "jest --testPathPattern=tests/api/webhooks",
    "test:security": "jest --testPathPattern=tests/security",
    "test:database": "jest --testPathPattern=tests/database",
    "test:integration": "jest --testPathPattern=tests/integration",
    "test:comprehensive": "ts-node tests/test-runner.ts",
    "test:ci": "npm run lint && npm run test:comprehensive"
  },
  "dependencies": {
    "@strapi/strapi": "^4.22.1",
    "@strapi/plugin-users-permissions": "^4.22.1",
    "@strapi/plugin-i18n": "^4.22.1",
    "@strapi/plugin-graphql": "^4.22.1",
    "@strapi/plugin-documentation": "^4.22.1",
    "@strapi/plugin-upload": "^4.22.1",
    "@strapi/provider-upload-aws-s3": "^4.22.1",
    "@strapi/provider-email-nodemailer": "^4.22.1",
    "pg": "^8.11.5",
    "redis": "^4.6.13",
    "sharp": "^0.33.3",
    "slugify": "^1.6.6",
    "uuid": "^9.0.1",
    "axios": "^1.6.8",
    "node-cron": "^3.0.3"
  },
  "devDependencies": {
    "typescript": "^5.4.5",
    "@types/node": "^20.12.7",
    "@types/uuid": "^9.0.8",
    "ts-node": "^10.9.2",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.12",
    "ts-jest": "^29.1.2",
    "supertest": "^6.3.4",
    "@types/supertest": "^6.0.2",
    "sqlite3": "^5.1.7",
    "nock": "^13.5.4",
    "@types/jsonwebtoken": "^9.0.6",
    "jsonwebtoken": "^9.0.2"
  },
  "engines": {
    "node": ">=18.0.0 <=20.x.x",
    "npm": ">=6.0.0"
  }
}